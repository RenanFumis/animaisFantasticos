{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "no-param-reassign": 0,
        "import/extensions": 0,
        "no-use-before-define": 0,
        "import/newline-after-import": 0,
        "import/first": 0
    }
}
